# GitHub Token
GITHUB_TOKEN=your_github_token_here

# Groq API Key
GROQ_API_KEY=your_groq_api_key_here

# Database Configuration
DATABASE_URL=postgresql://cesto_user:cesto_password@localhost:5432/cesto_ai
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=cesto_user
DB_PASSWORD=cesto_password
DB_NAME=cesto_ai

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Authentication
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=24h

# Application Ports
FRONTEND_PORT=4400
BACKEND_PORT=3400
AI_SERVICES_PORT=8001

# Environment
NODE_ENV=development

# AI Services
AI_MODEL_CACHE_TTL=3600
AI_PREDICTION_CONFIDENCE_THRESHOLD=0.7

# CORS
CORS_ORIGINS=http://localhost:4400,http://localhost:3400

# Email (Optional)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM_EMAIL=noreply@cesto.ai

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/cesto.log
